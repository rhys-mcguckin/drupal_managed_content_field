{#
/**
 * @file
 * Default theme implementation for a managed content dropbutton wrapper.
 *
 * Available variables:
 * - children: Contains the child elements of the managed content dropbutton menu.
 *
 * @see template_preprocess()
 * @see template_preprocess_managed_content_dropbutton_wrapper()
 *
 * @ingroup themeable
 */
#}
{% if children %}
  {% spaceless %}
    <div class="managed-content-dropbutton-wrapper">
      {{ children }}
    </div>
  {% endspaceless %}
{% endif %}
